{
  "name": "robo-skills",
  "version": "1.0.0",
  "homepage": "https://robobee16.github.io/robo-skills/build/",
  "private": true,
  "scripts": {
    "rm": "rm -rf node_modules && rm yarn.lock || rd /s /q node_modules",
    "yrn:update": "yarn upgrade-interactive --latest",
    "i": "yarn install",
    "prettier:format": "prettier --write './**/*.{jsx,ts,tsx,css,md,json}'",
    "configure-husky": "npx husky install && npx husky add .husky/pre-commit \"npx --no-install lint-staged\"",
    "------------------ ----------------------- ------------------": "",
    "start": "react-app-rewired start",
    "build": "react-app-rewired build && mv ./build/index.html ./",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "------------------ git ------------------": "",
    "git:add": "git add .",
    "git:commit": "git commit -a -m 'auto'",
    "git:push": "git push",
    "git:run": "yarn run git:add && yarn run git:commit && yarn run git:push"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.0",
    "@types/jest": "^28.1.1",
    "@types/node": "^17.0.42",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "node-sass": "^7.0.1",
    "react": "^18.1.0",
    "react-app-rewired": "2.2.1",
    "react-dom": "^18.1.0",
    "react-error-boundary": "^3.1.4",
    "react-router-dom": "6.3.0",
    "react-scripts": "^5.0.1",
    "rxjs": "^7.5.5",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-typescript": "^0.14.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.1",
    "pre-commit": "^1.2.2",
    "prettier": "^2.6.2",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.0.4",
    "typescript": "^4.7.3"
  },
  "pre-commit": [
    "build"
  ],

  
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
